.piechart
    h3 {{ measurement.title }}
    button.toggle(v-if="false"
                  v-for="r in resolutions"
                  type="button"
                  :class="{active: resolution == r.key}"
                  @click="resolution = r.key") {{r.title}}
    chart(v-if="data"
          type="piechart"
          :values="data")


    listtable(:values="data"
              :label="'Bezeichnung'")
